class devolvercambio{
     public static void main(String[] args){
        

        int cantidadPagar = 248;
        int cantidadEntregado = 300;
        int cambioFinal = cantidadEntregado - cantidadPagar;
        int cambio = cambioFinal;

        int billete1 = cambio/1;
        cambio %= 1;
        int billete2 = cambio/2;
        cambio %= 2;
        int billete5 = cambio/5;
        cambio %= 5;
        int billete10 = cambio/10;
        cambio %= 10;
        int billete20 = cambio/20;
        cambio %= 20;
        int billete50 = cambio/50;
        cambio %= 50;
        int billete100 = cambio/100;
        cambio %= 100;

        System.out.println("Cantidad a Pagar:"+ cantidadPagar);
        System.out.println("Cantidad para entregar:" + cantidadEntregado);
        System.out.println("Cambio a regresar:" + cambioFinal);
        System.out.println("Billete de 1: " + billete1);
        System.out.println("Billete de 2: " + billete2);
        System.out.println("Billete de 5: " + billete5);
        System.out.println("Billete de 10: " + billete10);
        System.out.println("Billete de 20: " + billete20);
        System.out.println("Billete de 50: " + billete50);
        System.out.println("Billete de 100: " + billete100);



    }

}
    
